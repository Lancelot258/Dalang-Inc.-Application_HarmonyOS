<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:id="$+id:backgroud"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical"
    ohos:background_element="$media:background"
    >
    <Text
        ohos:id="$+id:txt_show_down_filename"
        ohos:height="32vp"
        ohos:width="1500vp"
        ohos:top_margin="20vp"
        ohos:left_margin="250vp"
        ohos:text_size="20vp"
        ohos:text_color="#FF1EE010"
        ohos:text="filename"
        />
    <Image
        ohos:height="match_parent"
        ohos:width="match_parent"
        ohos:image_src="$media:back"
        ohos:visibility="hide"
        ohos:id="$+id:back_icon_img"
        />
    <Button
        ohos:id="$+id:setting"
        ohos:height="32vp"
        ohos:width="32vp"
        ohos:background_element="$media:setting"
        ohos:left_margin="980vp"
        ohos:layout_alignment="right|top"/>

    <DirectionalLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:id="$+id:upLayout"
        ohos:top_margin="260vp"
        ohos:height="80vp"
        ohos:width="match_content"
        ohos:orientation="horizontal"
        >
    </DirectionalLayout>
    <DirectionalLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:id="$+id:buttonLayoutFirst"
        ohos:top_margin="465vp"
        ohos:height="80vp"
        ohos:width="match_content"
        ohos:orientation="horizontal"
        >
        <Button
            ohos:id="$+id:anFang"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:anfang"
            ohos:left_margin="138vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:tianShou"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:tianshou"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:xianhua"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:xianhua"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:liyi"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:liyi"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:fashi"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:fashi"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
    </DirectionalLayout>
    <DirectionalLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:id="$+id:buttonLayoutSecond"
        ohos:top_margin="545vp"
        ohos:height="80vp"
        ohos:width="match_content"
        ohos:orientation="horizontal"
        >
        <Button
            ohos:id="$+id:fengshui"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:fengshui"
            ohos:left_margin="270vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:renxiang"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:renxiang"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:fuwu"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:fuwu"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
        <Button
            ohos:id="$+id:muxing"
            ohos:height="62vp"
            ohos:width="110vp"
            ohos:background_element="$media:muxing"
            ohos:left_margin="41vp"
            ohos:layout_alignment="bottom"
            />
    </DirectionalLayout>

</DependentLayout>